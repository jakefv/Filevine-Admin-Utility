var toggled = TRUE/FALSE;
var currentOrg = '';

function clickByContent("searchText"){
var aTags = document.getElementsByTagName("a");
var found;
for (var i = 0; i < aTags.length; i++) {
  if (aTags[i].textContent == searchText) {
    found = aTags[i];
    break;
  }
  found.click();
}

function getOrgs() {
    var orgs = document.querySelectorAll('[ng-click="selectOrg(org)"]');
        result = [];
    for (var i = 0; i < orgs.length; i++) {
        result.push(orgs[i].textContent);
}
    return result;
}

//Chrome Toggle logic.
On Toggle Click:
If toggle = FALSE {
      Look up current org name
      - On project Page
      - Or Current Customs editorPage
      currentOrg = Looked Up Org.
      toggle = true;
      toggleColor = RED;
} else if (toggle = TRUE) {
      currentOrg = '';
      toggle = FALSE;
      toggleColor = BLUE;
}

//Every time you navigate to a new page.
On page load:
If (url = filevine & toggle = TRUE){
clickByContent(currentOrg);
}
Check URL:
If (window.location.href === https://app.filevine.com/#/advanced){
If Toggle = TRUE {
clickByContent(currentOrg);
} else {
var orgArray = getOrgs();
var choice = '';
element OrgChooser.style.display = "none";
create element OrgNewPicker (and it's a dynamic dropdown thing)
put where old element was.
populate with dropdown thing.
button.onClick(
choice = what you chose;
contentByClick(choice);
);
}

